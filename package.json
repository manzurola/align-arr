{
  "name": "align-arr",
  "version": "0.7.0",
  "description": "Find the minimal edit path between two generic arrays.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/manzurola/align-arr.git",
  "author": "manzurola <guy.manzurola@gmail.com>",
  "license": "MIT",
  "private": false,
  "keywords": [
    "alignment",
    "edit-distance",
    "levenshtein",
    "similarity",
    "diff"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "prettier": "^2.6.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.0.3",
    "jest": "^27.5.1"
  },
  "files": [
    "lib/**/*"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
